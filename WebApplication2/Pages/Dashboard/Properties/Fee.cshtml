@page
@model FireBnBWeb.Pages.Dashboard.Properties.FeeModel
@{
}
<div>
	@if (Model.feeList != null)
	{
		@foreach (var fee in Model.feeList)
		{
			<p>Fee Type: @fee.Fee.Type</p>
			<p>Percentage: @fee.Fee.Percentage</p>
		}
	}
</div>
<form method="post">
	<div class="col-md-3">
		<label class="form-label">Add New Fee</label>
		<select class="form-select" id="feechoice" name="feechoice" required>

			<option selected disabled value="">Choose...</option>
			@foreach (var feeoption in Model.feeOptions)
			{
				<option for="feechoice" value="@feeoption.Value">@feeoption.Text</option>
			}

		</select>
		<div class="invalid-feedback">
			Please select a valid state.
		</div>
		<input asp-for="objFee.PropertyId" type="hidden" value="@Model.propertyid" />
	</div>
	<button type="submit" class="btn btn-primary form-control">Create</button>
</form>
<div>
	<a asp-page="Discount" asp-route-id="@Model.propertyid" class="btn btn-outline-primary border form-control">Add Discounts</a>
</div>